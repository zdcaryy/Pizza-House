<body>


<div id="app">
    <my-top v-bind:vip="<%=session%>">
        <div slot="member" class="userName" v-if="<%=session%>">
            <div class="userName-con">
                <span class="userText"><%=session%></span>
                <b id="oB"></b>
            </div>
            <ul class="dropdown-ul">
                <li><a href="#">个人资料</a></li>
                <li class="divider"></li>
                <li><a href="#">订单管理</a></li>
                <li class="divider"></li>
