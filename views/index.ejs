            </ul>
            <div class="userCard">
                <a class="navbar-brand" href="#">我的卡券(7)</a>
            </div>
        </div>
    </my-top>

    <div class="Body" id="Body">
        <!--轮播-->
        <div class="banner">
            <div class="pic">
                <ul>
                    <%=infor%>
                    <% infor.index.forEach(function (ob) {%>
                        <li> <img src=" <%= ob.images %>" alt="">
                    <%}) %>
                </ul>
            </div>
            <div class="btn">
                <button class="left"> < </button>
                <button class="right"> > </button>
            </div>
            <div class="tab">
                <ul>
                    <li>1</li>
                    <li>2</li>
                    <li>3</li>
                    <li>4</li>
                </ul>
            </div>
        </div>
        <!--特选-->
        <div class="select">
            <div class="top">
                <ul>
                    <li>
                        <a href="">
                            <img src="./images/texuan1.jpg" alt="">
                            <span>免外送费</span>
                        </a>
                    </li>
                    <li>
                        <a href="">
                            <img src="./images/texuan2.jpg" alt="">
                            <span>聚乐盒</span>
                        </a>
                    </li>
                    <li>
                        <a href="">
                            <img src="./images/texuan3.jpg" alt="">
                            <span>比萨</span>
                        </a>
                    </li>
                    <li>
                        <a href="">
                            <img src="./images/texuan4.jpg" alt="">
                            <span>小吃</span>
                        </a>
                    </li>
                    <li class="top-last-li">
                        <a href="">
                            <img src="./images/texuan5.png" alt="">
                            <span>全部餐饮</span>
                        </a>
                    </li>

                </ul>
            </div>
            <div class="bottom">
                <span class="left">当季特选</span>
                <span class="right">登录享更多优惠活动</span>
            </div>
        </div>
        <!--加入我们-->
        <div class="join">
            <div class="top"> </div>
            <div class="mid"><img src="./images/more.jpg" width="1200" height="100" alt=""></div>
            <div class="bottom"><img src="./images/join.jpg" width="1200" height="260" alt=""></div>
        </div>
        <!--downLoad-->
        <my-app></my-app>
    </div>
<my-foot></my-foot>
</div>
<script>

    'use strick';

    var vue = Vue({
        el:"#app"
    });

</script>
